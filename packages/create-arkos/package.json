    "prebuild": "pnpm --filter @arkos/shared build",
    "prepublishOnly": "pnpm build"
  },
  "author": "Uanela Como",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/uanela/arkos.git"
  },
