import arkos from 'arkos';

arkos.init({
  {{#if authentication.type}}
  authentication: {
    mode: '{{authentication.type}}',
    allowedUsernameFields: ['{{authentication.usernameField}}'],
  },
  {{/if}}
  {{#if validation.type}}
  validation: {
    resolver: '{{validation.type}}'
  },
  {{/if}}
});
