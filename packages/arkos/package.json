{
  "name": "arkos",
  "version": "1.2.4-beta",
  "description": "The Express & Prisma Framework For RESTful API",
  "main": "dist/cjs/exports/index.js",
  "module": "dist/es2020/exports/index.js",
  "types": "dist/types/exports/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/exports/index.d.ts",
      "import": "./dist/es2020/exports/index.js",
      "require": "./dist/cjs/exports/index.js"
    },
    "./prisma": {
      "types": "./dist/types/exports/prisma/index.d.ts",
      "import": "./dist/es2020/exports/prisma/index.js",
      "require": "./dist/cjs/exports/prisma/index.js"
    },
    "./auth": {
      "types": "./dist/types/exports/auth/index.d.ts",
      "import": "./dist/es2020/exports/auth/index.js",
      "require": "./dist/cjs/exports/auth/index.js"
    },
    "./services": {
      "types": "./dist/types/exports/services/index.d.ts",
      "import": "./dist/es2020/exports/services/index.js",
      "require": "./dist/cjs/exports/services/index.js"
    },
    "./controllers": {
      "types": "./dist/types/exports/controllers/index.d.ts",
      "import": "./dist/es2020/exports/controllers/index.js",
      "require": "./dist/cjs/exports/controllers/index.js"
    },
    "./middlewares": {
      "types": "./dist/types/exports/middlewares/index.d.ts",
      "import": "./dist/es2020/exports/middlewares/index.js",
      "require": "./dist/cjs/exports/middlewares/index.js"
    },
    "./validation": {
      "types": "./dist/types/exports/validation/index.d.ts",
      "import": "./dist/es2020/exports/validation/index.js",
      "require": "./dist/cjs/exports/validation/index.js"
    },
    "./error-handler": {
      "types": "./dist/types/exports/error-handler/index.d.ts",
      "import": "./dist/es2020/exports/error-handler/index.js",
      "require": "./dist/cjs/exports/error-handler/index.js"
    },
    "./utils": {
      "types": "./dist/types/exports/utils/index.d.ts",
      "import": "./dist/es2020/exports/utils/index.js",
      "require": "./dist/cjs/exports/utils/index.js"
    },
    "./cli": {
      "import": "./dist/es2020/utils/cli/index.js",
      "require": "./dist/cjs/utils/cli/index.js"
    },
    "./*": {
      "types": "./dist/types/exports/*.d.ts",
      "import": "./dist/es2020/exports/*.js",
      "require": "./dist/cjs/exports/*.js"
    }
  },
  "bin": {
    "arkos": "./cli.js"
  },
  "typesVersions": {
    "*": {
      "*": ["./dist/types/exports/*/index.d.ts"],
      "services": ["./dist/types/exports/services/index.d.ts"],
      "validation": ["./dist/types/exports/validation/index.d.ts"],
      "error-handler": ["./dist/types/exports/error-handler/index.d.ts"],
      "prisma": ["./dist/types/exports/prisma/index.d.ts"],
      "auth": ["./dist/types/exports/auth/index.d.ts"],
      "middlewares": ["./dist/types/exports/middlewares/index.d.ts"],
      "utils": ["./dist/types/exports/utils/index.d.ts"]
    }
  },
  "scripts": {
    "build:es2020": "tsc -p tsconfig.es2020.json",
    "build:types": "tsc -p tsconfig.types.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "postbuilda": "echo {\\\"type\":\"commonjs\\\"} > dist/cjs/package.json",
    "build": "npx rimraf dist && pnpm run build:cjs && pnpm run build:es2020 && pnpm run build:types && pnpm run minify",
    "minify": "esbuild dist/**/*.js --minify --outdir=dist --allow-overwrite --log-limit=0 --define:process.env.NODE_ENV=process.env.NODE_ENV",
    "clean": "rm -r dist",
    "dev": "ts-node --watch",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "pnpm run build",
    "prelinkOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ui": "vitest --ui",
    "test:create": "ts-node src/scripts/create-test.ts",
    "publish:test": "npm publish --tag test",
    "publish:beta": "npm publish --tag beta",
    "publish": "npm publish"
  },
  "keywords": [
    "auto API routes",
    "TypeScript package",
    "API generator",
    "authentication",
    "email service",
    "error handling",
    "file upload",
    "image optimization",
    "Prisma integration",
    "customizable API",
    "auto routing",
    "file upload routes",
    "dynamic API routes",
    "Prisma ORM",
    "image upload",
    "API middleware"
  ],
  "author": "Uanela Como",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/uanela/arkos.git"
  },
  "bugs": {
    "url": "https://github.com/uanela/arkos/issues"
  },
  "homepage": "https://www.arkosjs.com",
  "files": ["dist", "cli.js", "README.md"],
  "devDependencies": {
    "@prisma/client": "^6.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/html-to-text": "^9.0.4",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.13",
    "@types/nodemailer": "^6.4.17",
    "@types/pluralize": "^0.0.33",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "esbuild": "^0.25.1",
    "jest": "^29.7.0",
    "mimetype": "^0.0.8",
    "openapi-types": "^12.1.3",
    "prisma": "^6.4.1",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "chokidar": "^4.0.3",
    "class-validator-jsonschema": "^5.0.2",
    "commander": "^13.1.0",
    "pluralize": "^8.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod-to-json-schema": "^3.24.6"
  },
  "peerDependencies": {
    "@prisma/client": "^6.4.1",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "mimetype": "^0.0.8",
    "multer": "2.0.1",
    "nodemailer": "^6.10.0",
    "sharp": "^0.33.5",
    "zod": "^3.24.2"
  },
  "engines": {
    "node": ">=14.16"
  },
  "pnpm": {
    "overrides": {},
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "@scarf/scarf",
      "esbuild",
      "prisma",
      "sharp"
    ]
  }
}
